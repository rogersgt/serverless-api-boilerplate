{
  "extends": ["eslint-config-google"],
  "env": {
    "node": true
  },
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 2021
  },
  "rules": {
    "indent": ["error", 2],
    "require-jsdoc": 0,
    "object-curly-spacing": ["error", "always"],
    "max-len": ["error", { "code": 124 }],
    "semi": 0
  }
}